<Window x:Class="MidiActivation.Window1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Midi Activation" Height="223" Width="430" ResizeMode="NoResize">
    <Grid>
		<TextBox Height="23" Margin="135,61,23,0" Name="serial_number" VerticalAlignment="Top" />
		<Label Height="28" Margin="17,59,0,0" Name="label1" VerticalAlignment="Top" HorizontalAlignment="Left" Width="106">Serial number</Label>
		<TextBox Height="23" Margin="135,0,23,60" Name="activation_value" VerticalAlignment="Bottom" IsReadOnly="True" />
		<Label Height="28" HorizontalAlignment="Left" Margin="17,0,0,57" Name="label2" VerticalAlignment="Bottom" Width="106">Activation value</Label>
		<Button Height="23" HorizontalAlignment="Right" Margin="0,0,23,16" Name="calculate_button" VerticalAlignment="Bottom" Width="105" Click="calculate_button_Click">Calculate</Button>
		<Label Height="28" Margin="17,15,105,0" Name="label3" VerticalAlignment="Top">Insert Serial number. For example: 1432</Label>
	</Grid>
</Window>
